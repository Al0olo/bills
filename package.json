{
  "name": "bills",
  "version": "1.0.0",
  "description": "Microservices-based subscription billing system with NestJS, Prisma, and PostgreSQL",
  "main": "index.js",
  "scripts": {
    "start": "nx serve subscription-service",
    "start:payment": "nx serve payment-service",
    "start:all": "nx run-many --target=serve --projects=subscription-service,payment-service --parallel",
    "build": "nx build subscription-service && nx build payment-service",
    "build:subscription": "nx build subscription-service",
    "build:payment": "nx build payment-service",
    "test": "nx test",
    "test:all": "nx run-many --target=test --all",
    "test:subscription": "nx test subscription-service",
    "test:payment": "nx test payment-service",
    "test:cov": "nx run-many --target=test --all --coverage",
    "lint": "nx lint",
    "lint:all": "nx run-many --target=lint --all",
    "lint:subscription": "nx lint subscription-service",
    "lint:payment": "nx lint payment-service",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "prisma:generate": "pnpm prisma:generate:subscription && pnpm prisma:generate:payment",
    "prisma:generate:subscription": "prisma generate --schema=./apps/subscription-service/prisma/schema.prisma",
    "prisma:generate:payment": "prisma generate --schema=./apps/payment-service/prisma/schema.prisma",
    "prisma:migrate:subscription": "prisma migrate dev --schema=./apps/subscription-service/prisma/schema.prisma",
    "prisma:migrate:payment": "prisma migrate dev --schema=./apps/payment-service/prisma/schema.prisma",
    "prisma:migrate:deploy:subscription": "prisma migrate deploy --schema=./apps/subscription-service/prisma/schema.prisma",
    "prisma:migrate:deploy:payment": "prisma migrate deploy --schema=./apps/payment-service/prisma/schema.prisma",
    "prisma:studio:subscription": "prisma studio --schema=./apps/subscription-service/prisma/schema.prisma",
    "prisma:studio:payment": "prisma studio --schema=./apps/payment-service/prisma/schema.prisma",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:rebuild": "docker-compose up -d --build",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "db:reset:subscription": "prisma migrate reset --schema=./apps/subscription-service/prisma/schema.prisma --force",
    "db:reset:payment": "prisma migrate reset --schema=./apps/payment-service/prisma/schema.prisma --force",
    "seed": "echo 'Seed scripts will be implemented later'",
    "clean": "nx reset && rm -rf dist node_modules",
    "affected:test": "nx affected --target=test",
    "affected:build": "nx affected --target=build",
    "affected:lint": "nx affected --target=lint",
    "graph": "nx graph",
    "prepare": "pnpm prisma:generate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/devkit": "^22.0.2",
    "@nx/eslint": "^22.0.2",
    "@nx/eslint-plugin": "22.0.2",
    "@nx/jest": "^22.0.2",
    "@nx/js": "^22.0.2",
    "@nx/nest": "^22.0.2",
    "@nx/node": "^22.0.2",
    "@nx/web": "22.0.2",
    "@nx/webpack": "22.0.2",
    "@nx/workspace": "^22.0.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "^22.0.2",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.40.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.18.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^11.0.0",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crypto-js": "^4.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "uuid": "^13.0.0"
  }
}
